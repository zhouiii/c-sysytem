<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.temptation.dao.BoUserDao">
    <select id="findByUsername" parameterType="string" resultType="BoUser">
        SELECT * FROM bo_user WHERE USER_ACCOUNT=#{userAccount}
    </select>
</mapper>